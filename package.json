{
    "name": "transbooleq",
    "version": "2.0.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "A transformation tool to replace Boolean equalities by equational constraints",
    "category": [ "Analysis", "Optimization" ],
    "dependencies": {
        "base"         : ">= 1.0.0, < 2.0.0",
        "cass-analysis": ">= 2.0.0",
        "cass"         : ">= 2.0.0",
        "csv"          : ">= 1.0.0",
        "flatcurry"    : ">= 2.0.0"
    },
    "compilerCompatibility": {
        "pakcs": ">= 2.0.0",
        "kics2": ">= 2.0.0"
    },
    "executable": {
        "name": "curry-transbooleq",
        "main": "BindingOpt"
    },
    "testsuite": {
        "src-dir": "examples",
        "modules": [ "Expressions", "Grep", "Half", "Last", "Various" ]
    },
     "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/transbooleq.git",
        "tag": "$version"
    }
}
